{
  "name": "game",
  "description": "Game",
  "version": "0.0.1",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/maxtream/game"
  },
  "scripts": {
    "clean": "rm -rf ./public/*",
    "jslint": "node_modules/.bin/eslint ./src/**/*.{js,vue}",
    "stylelint": "node_modules/.bin/stylelint \"./**/*.scss\" --syntax scss",
    "webpack": "node_modules/.bin/webpack --progress --config webpack.config.js",
    "build": "yarn clean && yarn jslint && yarn stylelint && yarn webpack",
    "release": "git pull && yarn && yarn jslint && yarn stylelint && node_modules/.bin/webpack --config webpack.config.js --env.production",
    "watch": "node_modules/.bin/webpack-dev-server"
  },
  "dependencies": {
    "@babel/core": "7.1.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "axios": "0.18.0",
    "babel-loader": "8.0.2",
    "bootstrap": "4.1.3",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "1.0.0",
    "cssnano": "4.1.0",
    "eslint": "5.6.0",
    "eslint-plugin-vue": "4.7.1",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "font-awesome": "4.7.0",
    "hammerjs": "2.0.8",
    "html-webpack-harddisk-plugin": "0.2.0",
    "html-webpack-plugin": "3.2.0",
    "image-loader": "0.0.1",
    "node-sass": "4.9.3",
    "node-sass-glob-importer": "5.2.0",
    "postcss-loader": "3.0.0",
    "raw-loader": "0.5.1",
    "replace-in-file-webpack-plugin": "1.0.6",
    "sass": "1.14.0",
    "sass-loader": "7.1.0",
    "socket.io": "^2.3.0",
    "stylelint": "9.5.0",
    "uglifyjs-webpack-plugin": "2.0.1",
    "vue": "2.5.17",
    "vue-analytics": "5.16.0",
    "vue-awesome-swiper": "3.1.3",
    "vue-i18n": "^8.2.1",
    "vue-loader": "15.4.2",
    "vue-multiselect": "2.1.0",
    "vue-router": "3.0.1",
    "vue-template-compiler": "2.5.17",
    "vuex": "3.0.1",
    "webpack": "4.19.1",
    "webpack-cli": "3.1.0",
    "webpack-dashboard": "2.0.0",
    "webpack-dev-server": "3.1.8"
  },
  "devDependencies": {
    "git-commit-msg": "0.3.0",
    "pre-commit": "1.2.2"
  },
  "pre-commit": [
    "jslint",
    "stylelint"
  ],
  "git-commit-hook": {
    "types": [
      "feat",
      "fix",
      "chore",
      "docs",
      "refactor",
      "style",
      "test",
      "revert",
      "merge"
    ],
    "lineLength": 72
  }
}

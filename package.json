{
  "name": "roidle.com",
  "description": "Ragnarok Idle",
  "version": "0.8.8",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxorlovsky/roidle.com"
  },
  "scripts": {
    "clean": "rm -rf ./public/*",
    "jslint": "node_modules/.bin/eslint ./src/**/*.{js,vue}",
    "stylelint": "node_modules/.bin/stylelint \"./**/*.scss\" --syntax scss",
    "webpack": "node_modules/.bin/webpack --progress --config webpack.config.js",
    "build": "yarn clean && yarn jslint && yarn stylelint && yarn webpack",
    "release": "git pull && yarn && yarn jslint && yarn stylelint && node_modules/.bin/webpack --config webpack.config.js --env production",
    "watch": "node_modules/.bin/webpack serve",
    "watch:local": "node_modules/.bin/webpack serve --env local"
  },
  "dependencies": {
    "@babel/core": "7.13.14",
    "@babel/preset-env": "7.13.12",
    "axios": "0.21.1",
    "babel-loader": "8.2.2",
    "babel-minify-webpack-plugin": "0.3.1",
    "bootstrap": "4.5.0",
    "copy-webpack-plugin": "7.0.0",
    "core-js": "3.10.0",
    "css-loader": "5.2.0",
    "cssnano": "4.1.10",
    "date-fns": "2.19.0",
    "eslint": "7.23.0",
    "eslint-plugin-vue": "7.8.0",
    "express": "4.17.1",
    "file-loader": "6.2.0",
    "firebase": "8.3.2",
    "hammerjs": "2.0.8",
    "html-webpack-harddisk-plugin": "2.0.0",
    "html-webpack-plugin": "5.3.1",
    "image-loader": "0.0.1",
    "mini-css-extract-plugin": "1.4.0",
    "node-sass": "5.0.0",
    "node-sass-glob-importer": "5.3.2",
    "postcss-loader": "5.2.0",
    "raw-loader": "4.0.2",
    "replace-in-file-webpack-plugin": "1.0.6",
    "sass": "1.32.8",
    "sass-loader": "11.0.1",
    "socket.io-client": "4.0.1",
    "stylelint": "13.12.0",
    "vue": "2.6.12",
    "vue-analytics": "5.22.1",
    "vue-loader": "15.9.6",
    "vue-multiselect": "2.1.6",
    "vue-range-slider": "0.6.0",
    "vue-router": "3.5.1",
    "vue-template-compiler": "2.6.12",
    "vue-toggles": "1.1.3",
    "vuex": "3.6.2",
    "webpack": "5.29.0",
    "webpack-cli": "4.6.0",
    "webpack-dev-server": "3.11.2"
  },
  "devDependencies": {
    "git-commit-msg": "0.3.0",
    "pre-commit": "1.2.2"
  },
  "pre-commit": [
    "jslint",
    "stylelint"
  ],
  "git-commit-hook": {
    "types": [
      "feat",
      "fix",
      "chore",
      "docs",
      "refactor",
      "style",
      "test",
      "revert",
      "merge"
    ],
    "lineLength": 120
  }
}
